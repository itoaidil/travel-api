# Database Configuration
DB_HOST=localhost
DB_USER=root
DB_PASSWORD=
DB_NAME=travel_booking
DB_PORT=3306

# Server Configuration
PORT=3000
NODE_ENV=development

# JWT Secret (generate random string for production)
JWT_SECRET=your-secret-key-here

# Midtrans Payment Gateway Configuration
# Daftar di: https://dashboard.midtrans.com/
# Sandbox (testing): https://dashboard.sandbox.midtrans.com/
MIDTRANS_ENVIRONMENT=sandbox
MIDTRANS_SERVER_KEY=YOUR_SANDBOX_SERVER_KEY
MIDTRANS_CLIENT_KEY=YOUR_SANDBOX_CLIENT_KEY

# Frontend URL (untuk callback payment)
FRONTEND_URL=http://localhost:3000
